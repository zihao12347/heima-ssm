<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.ssm.dao.IMemberDao">
    <select id="findById" resultType="member">
        select * from member where id = #{id}
    </select>
</mapper>